    bo_name CDATA #IMPLIED
    bo_current_ref CDATA #IMPLIED
    bo_mode CDATA #IMPLIED
    bo_teamtype CDATA #IMPLIED
>

<!ELEMENT bo_team EMPTY>
<!ATTLIST bo_team
