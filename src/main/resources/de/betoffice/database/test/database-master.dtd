    bo_email CDATA #IMPLIED
    bo_phone CDATA #IMPLIED
    bo_password CDATA #IMPLIED
    bo_automat CDATA #IMPLIED
    bo_excluded CDATA #IMPLIED
    bo_title CDATA #IMPLIED
>

<!ELEMENT bo_user_season EMPTY>
<!ATTLIST bo_user_season
    id CDATA #REQUIRED
