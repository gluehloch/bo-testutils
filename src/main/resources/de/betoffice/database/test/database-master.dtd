<!ELEMENT dataset (
    bo_grouptype*,
    bo_team*,
    bo_teamalias*,
    bo_user*)>

<!ELEMENT bo_game EMPTY>
<!ATTLIST bo_game
    id CDATA #REQUIRED
    bo_date CDATA #IMPLIED
    bo_group_ref CDATA #IMPLIED
    bo_hometeam_ref CDATA #IMPLIED
    bo_guestteam_ref CDATA #IMPLIED
    bo_homegoals CDATA #IMPLIED
    bo_guestgoals CDATA #IMPLIED
    bo_isplayed CDATA #IMPLIED
    bo_gamelist_ref CDATA #IMPLIED
    bo_index CDATA #IMPLIED
    bo_datetime CDATA #IMPLIED
>

<!ELEMENT bo_gamelist EMPTY>
<!ATTLIST bo_gamelist
    id CDATA #REQUIRED
    bo_index CDATA #IMPLIED
    bo_date CDATA #IMPLIED
    bo_season_ref CDATA #IMPLIED
    bo_group_ref CDATA #IMPLIED
    bo_datetime CDATA #IMPLIED
>

<!ELEMENT bo_gametipp EMPTY>
<!ATTLIST bo_gametipp
    id CDATA #REQUIRED
    bo_homegoals CDATA #IMPLIED
    bo_guestgoals CDATA #IMPLIED
    bo_user_ref CDATA #IMPLIED
    bo_game_ref CDATA #IMPLIED
    bo_status CDATA #IMPLIED
    bo_tipps_index CDATA #IMPLIED
>

<!ELEMENT bo_group EMPTY>
<!ATTLIST bo_group
    id CDATA #REQUIRED
